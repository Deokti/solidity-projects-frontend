{
	"extends": [
		"stylelint-config-standard",
		"stylelint-order-config-standard"
	],
	"plugins": [
		"stylelint-order"
	],
	"rules": {
		"indentation": [
			2
		],
		"color-hex-case": "upper",
		"color-function-notation": "legacy",
		"alpha-value-notation": "number",
		"selector-class-pattern": null,
		"at-rule-no-unknown": null
	}
}